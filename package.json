		"@types/react-dom": "^17.0.5",
		"@types/react-modal": "^3.12.0",
		"@types/react-redux": "^7.1.16",
		"@types/react-virtualized": "^9.21.21",
		"autoprefixer": "^9",
		"canvas": "^2.7.0",
		"clsx": "^1.1.1",
